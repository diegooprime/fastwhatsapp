<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.wapp.bridge</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/p/.local/bin/whatsapp-bridge</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/p/.whatsapp-raycast</string>

    <key>KeepAlive</key>
    <true/>

    <key>RunAtLoad</key>
    <true/>

    <key>ThrottleInterval</key>
    <integer>10</integer>

    <key>StandardOutPath</key>
    <string>/Users/p/.whatsapp-raycast/bridge.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/p/.whatsapp-raycast/bridge.log</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>HOME</key>
        <string>/Users/p</string>
    </dict>
</dict>
</plist>
